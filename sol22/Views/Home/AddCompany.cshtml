@model sol22.Models.Company


<h2>Add Company</h2>

<form asp-action="SaveCompany" method="post">
    <div>
        <label asp-for="Name"></label>
        <input asp-for="Name" />
    </div>
    <div>
        <label asp-for="Address"></label>
        <input asp-for="Address" />
    </div>

    <button type="submit">Add</button>
</form>